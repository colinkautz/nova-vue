{
    "name": "nova-vue",
    "version": "1.5.0",
    "description": "Vue support for Nova editor.",
    "main": "src/Scripts/main.js",
    "scripts": {
        "build": "npx rollup -c && cp CHANGELOG.md README.md LICENSE Vue.novaextension",
        "watch": "onchange -i \"src/**/*.js\" \"src/**/*.json\" -- npm run build",
        "publish": "npx rollup -v && cp CHANGELOG.md README.md LICENSE Vue.novaextension",
        "develop": "rollup -c"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tommasongr/nova-vue.git"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tommasongr/nova-vue/issues"
    },
    "homepage": "https://github.com/tommasongr/nova-vue#readme",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^15.0.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "eslint": "^7.12.0",
        "eslint-config-prettier": "^6.14.0",
        "eslint-plugin-nova": "^1.2.0",
        "eslint-plugin-prettier": "^3.1.4",
        "onchange": "^7.0.2",
        "rollup": "^2.26.11"
    },
    "dependencies": {
        "nova-extension-utils": "^1.2.1"
    }
}
